<div class="content container">
    <div class="page-wrapper">
      <header class="page-heading clearfix">
        <h1 class="heading-title pull-left">Catalog</h1>
        <div class="breadcrumbs pull-right">
          <ul class="breadcrumbs-list">
              <li class="breadcrumbs-label">You are here:</li>
              <li><a href="index.html">Home</a><i class="fa fa-angle-right"></i></li>
              <li><a href="#">Catalog</a><i class="fa fa-angle-right"></i></li>
              <li class="current">Catalog</li>
          </ul>
        </div>
      </header>
      <div class="page-content"></div>
        <div class="row page-row">
          <aside class="page-sidebar col-md-2 col-sm-4 affix-top">
            <section class="widget">
                <ul class="nav">
                  <li class="active"><a href="/catalog/index">Catalog</a></li>
                  <li><a href="/catalog/brochure">Brochure</a></li>
                </ul>
                </section><!--//widget-->
          </aside>
          <div class="content-wrapper col-md-10 col-sm-8">
            <div class="page-content">
            <div class="row">
            <% @catalog.each do |catalog| %>
              <% if catalog.catalogtype == "image"%>
              <div class="col-md-3 col-sm-3 col-xs-6 text-center">
                <div class="album-cover">
                  <a href="<%= catalog.path.url %>" download><%= image_tag catalog.path_url, class: "img-responsive fixed-img-height" %></a>
                  <div class="desc">
                    <h4>
                      <a href="<%= catalog.path.url %>" download><%= catalog.title %></a>
                    </h4>
                  </div>
                </div>
              </div>
              <% end %>
            <% end %>
            </div>
          </div>
      </div>
    </div>
  </div>
</div>



